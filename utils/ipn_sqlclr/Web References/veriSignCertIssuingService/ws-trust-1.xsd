    <xs:sequence>
      <xs:any minOccurs="1" maxOccurs="1" namespace="##other" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="AllowPostdating" type="wst:AllowPostdatingType" />
  <xs:complexType name="AllowPostdatingType" />
